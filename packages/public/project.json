{
  "namedInputs": {
    "build": [
      "{projectRoot}/**/*",
      "!{projectRoot}/node_modules/**/*",
      "!{projectRoot}/.next/**/*"
    ]
  },
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "inputs": ["build"],
      "outputs": ["{projectRoot}/.next", "{workspaceRoot}/docs"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "executor": "nx:run-script",
      "options": {
        "script": "dev"
      },
      "dependsOn": ["^build"]
    }
  }
}
