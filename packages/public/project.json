{
  "namedInputs": {
    "build": [
      "{projectRoot}/**/*",
      "!{projectRoot}/node_modules/**/*",
      "!{projectRoot}/.next/**/*"
    ],
    "export": ["{projectRoot}/.next/**/*"]
  },
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      },
      "inputs": ["build"],
      "outputs": ["{projectRoot}/.next"],
      "dependsOn": ["^build"]
    },
    "export": {
      "executor": "nx:run-script",
      "options": {
        "script": "export"
      },
      "outputs": ["{workspaceRoot}/docs"],
      "inputs": ["export"],
      "dependsOn": ["build"]
    },
    "dev": {
      "executor": "nx:run-script",
      "options": {
        "script": "dev"
      },
      "dependsOn": ["^build"]
    }
  }
}
